# The Authority - Environment Configuration
# Copy this file to .env and fill in your actual values

# Database Configuration
MYSQL_DSN="mysql://user:pass@127.0.0.1/authority?charset=utf8mb4"

# Discord Bot Configuration
DISCORD_TOKEN="your_discord_bot_token_here"
DISCORD_GUILD_ID="your_discord_server_id_here"

# FiveM Server Configuration
FIVEM_LICENSE_KEY="your_fivem_license_key_here"
STEAM_WEB_API_KEY="your_steam_web_api_key_here"

# Optional: pma-voice
VOICE_USE_SENDING_RANGE_ONLY=true
VOICE_ENABLE_PROXIMITY_CYCLE=1
VOICE_DEFAULT_CYCLE="F11"
VOICE_ENABLE_RADIOS=1
VOICE_ENABLE_CALLS=1

# Optional: Additional Configuration
# TXADMIN_PORT=40120
# SERVER_PORT=30120

